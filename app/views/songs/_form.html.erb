<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <% if !@song.artist %>
    <%= f.select(:artist_id, Artist.all.collect {|u| [u.name, u.id]}) %>

  <% end %>
  <%= f.submit %>
<% end %>
